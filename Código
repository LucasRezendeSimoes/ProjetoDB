CREATE DATABASE projeto;
Tabelas
CREATE TABLE Alunos(
  Nome VARCHAR(255),
  RA VARCHAR(255),
  Curso Varchar(255),
  Semestre int,
  Tot_cred int,
  Tcc_id VARCHAR(10)
);
CREATE TABLE Professores(
  Nome VARCHAR(255),
  P_id VARCHAR(255),
  Departamento VARCHAR(255),
  Disciplina VARCHAR(255)
);
CREATE TABLE Cursos(
  Nome VARCHAR(255),
  C_id VARCHAR(255),
  Coordenador Varchar(255),
);
CREATE TABLE Departamentos(
  Nome VARCHAR(255),
  D_id VARCHAR(255),
  Id_chefe Varchar(255),
);
CREATE TABLE Disciplinas(
  Nome VARCHAR(255),
  Dc_id VARCHAR(255),
  Departamento Varchar(255),
);
CREATE TABLE MCC(
);


CREATE TABLE TCC(
  Tcc_id VARCHAR(255),
  P_id VARCHAR(255),
);
CREATE TABLE Hist_a(
  RA VARCHAR(255),
  C_id VARCHAR(255),
  semestre int,
  ano int,
  nota int
);
CREATE TABLE Hist_p(
  P_id VARCHAR(255),
  C_id VARCHAR(255),
  semestre int,
  ano int,
);


Valores para teste
insert into alunos 
values ('Joãozinho123anti_lacração', '123456789', 'Eng. Prod', 10);



Principais
alunos (nome, RA, Curso, Semestre, tot_cred, tcc_id)
professores (nome, P_id, departamento, disciplina) 
cursos (nome, id, coordenador)
departamentos(nome, id, id_chefe)
disciplinas (nome, id, Departamento) 

matriz curricular dos cursos

Extras
TCC(tcc_id, id professor)
hist_a(RA, id curso, semestre, ano, nota)
hist_p(id professor, id curso, semestre, ano)
