CREATE DATABASE projeto;
Tabelas
CREATE TABLE Alunos(
  Nome VARCHAR(255),
  RA VARCHAR(255),
  id_Curso Varchar(255),
  Semestre int,
  Tot_cred int,
  id_Tcc VARCHAR(10),
  PRIMARY KEY(RA),
);
CREATE TABLE Professores(
  Nome VARCHAR(255),
  id VARCHAR(255),
  Depart_id VARCHAR(255),
  PRIMARY KEY(id),
);
CREATE TABLE Cursos(
  Nome VARCHAR(255),
  id VARCHAR(255),
  Coordenador Varchar(255),
  PRIMARY KEY(id),
);
CREATE TABLE Departamentos(
  Nome VARCHAR(255),
  id VARCHAR(255),
  id_chefe Varchar(255),
  PRIMARY KEY(id),
);
CREATE TABLE Disciplinas(
  Nome VARCHAR(255),
  id VARCHAR(255),
  Depart_id Varchar(255),
  PRIMARY KEY(id),
);
CREATE TABLE MCC(
  id_curso VARCHAR(255),
  semestre int,
  Nome_Curso VARCHAR(255),
  id_disc VARCHAR(255),
  Nome_disc VARCHAR(255),
  PRIMARY KEY(id_curso),
);

CREATE TABLE TCC(
  Tcc_id VARCHAR(255),
  id_professor VARCHAR(255),
  PRIMARY KEY(Tcc_id),
);
CREATE TABLE Hist_a(
  id_aluno VARCHAR(255),
  id_curso VARCHAR(255),
  id_disciplinas VARCHAR(255),
  semestre int,
  ano int,
  nota int
  PRIMARY KEY(id_aluno),
);
CREATE TABLE Hist_p(
  id_professor VARCHAR(255),
  id_disciplina VARCHAR(255),
  id_curso VARCHAR(255),
  semestre int,
  ano int,
  PRIMARY KEY(id_professor),
);
CREATE TABLE Ensina(
  id_professor VARCHAR(255),
  id_disciplina VARCHAR(255),
  PRIMARY KEY(id_professor),
);

ALTER TABLE Alunos(
  ADD CONSTRAINT FOREIGN KEY (id_curso) REFERENCES Cursos(id),
  ADD CONSTRAINT FOREIGN KEY (id_Tcc) REFERENCES TCC(Tcc_id)
);
ALTER TABLE Professores(
  ADD CONSTRAINT FOREIGN KEY (Depart_id) REFERENCES Departamentos(id),
);
ALTER TABLE Cursos(
  ADD CONSTRAINT FOREIGN KEY (Coordenador) REFERENCES Professores(id)
);
ALTER TABLE Departamentos(
  ADD CONSTRAINT FOREIGN KEY (id_chefe) REFERENCES Professores(id)
);
ALTER TABLE Disciplinas(
  ADD CONSTRAINT FOREIGN KEY (Depart_id) REFERENCES Departamentos(id)
);
ALTER TABLE MCC(
  ADD CONSTRAINT FOREIGN KEY (id_Curso) REFERENCES Cursos(id),
  ADD CONSTRAINT FOREIGN KEY (id_disc) REFERENCES Disciplinas(id),
  ADD CONSTRAINT FOREIGN KEY (Nome_disc) REFERENCES Disciplinas(id)
);

ALTER TABLE TCC(
 ADD CONSTRAINT FOREIGN KEY (id_professor) REFERENCES Professores(id)
);
ALTER TABLE Hist_a(
  ADD CONSTRAINT FOREIGN KEY (id_aluno) REFERENCES Alunos(RA),
  ADD CONSTRAINT FOREIGN KEY (id_curso) REFERENCES Cursos(id),
  ADD CONSTRAINT FOREIGN KEY (id_disciplinas) REFERENCES Disciplinas(id)
);
ALTER TABLE Hist_p(
  ADD CONSTRAINT FOREIGN KEY (id_professor) REFERENCES Professores(id),
  ADD CONSTRAINT FOREIGN KEY (id_curso) REFERENCES Cursos(id),
  ADD CONSTRAINT FOREIGN KEY (id_disciplinas) REFERENCES Disciplinas(id)
);
ALTER TABLE Ensina(
  ADD CONSTRAINT FOREIGN KEY (id_professor) REFERENCES Professores(id),
  ADD CONSTRAINT FOREIGN KEY (id_disciplinas) REFERENCES Disciplinas(id)
);

